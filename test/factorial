:Loop
push 0                      #
pop rbx                     #можно оптимизировать

:PrepareB4Factorial

push 1      #               #
push rbx    #rbx += 1       #
add         #               #тяжело оптимизировать
pop rbx     #               #

push 1      #               #
pop rcx     #counter        #можно оптимизировать

push 1      #               #
pop rax     #answer         #можно оптимизировать

:Factorial

push rcx                    #
push 10                     #
je Exit                     #можно оптимизировать (сразу в регистры)

push rax    #               #
pop         #               #

push rcx    #               #
push 1      #counter += 1   #
add         #               #тяжело оптимизировать
pop rcx     #               #

push rax    #               #
push rcx    #answ *= count  #
mul         #               #тяжело оптимизировать
pop rax     #               #

jmp Factorial

:Exit

push rbx                    #
push 1000                   #можно оптимизировать
jne PrepareB4Factorial      #

hlt
